/* pages/detail/detail.wxss */
@font-face {
  font-family: 'iconfont';  /* project id 681496 */
  src: url('//at.alicdn.com/t/font_681496_4irqic6iazag7gb9.eot');
  src: url('//at.alicdn.com/t/font_681496_4irqic6iazag7gb9.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_681496_4irqic6iazag7gb9.woff') format('woff'),
  url('//at.alicdn.com/t/font_681496_4irqic6iazag7gb9.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_681496_4irqic6iazag7gb9.svg#iconfont') format('svg');
}

.d_iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.d_icon-xiangxiajiantou:before { content: "\e61f"; }

.d_icon-xiangshangjiantou:before { content: "\e620"; }


.d_view{
  width:100%;
  height:100%;
  background-color:#FCFCFC;
}
.d_indexImg{
  width:calc(100% - 50rpx);
  padding:25rpx;
  height:392rpx;
  background-color: #fff;
}
.d_indexImg2{
  width:100%;
  padding-top:25rpx;
  height:320rpx;
  background-color: #fff;
}
.d_indexImg image,.d_indexImg2 image{
  width:100%;
  height:100%;
}
.swiper{
  height: 392rpx;
}
.d_overtime{
  width: 100%;
  height: 44rpx;
  background: rgb(230, 230, 230);
  padding-left: 25rpx;
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: rgb(119, 119, 119);
}
.d_active{
  color: red;
}
